   "source": [
    "def birthday(s, d, m):\n",
    "    result = 0\n",
    "    for i in range(len(s)-m+1):\n",
    "        sum = 0\n",
    "        j = i\n",
    "        while j < i+m:\n",
    "            sum += s[j]\n",
    "            j+=1\n",
    "        result += 1 if sum == d else 0\n",
    "    return result"
   ]
